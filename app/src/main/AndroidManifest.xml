<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.binkery.itarget">

    <application
        android:name=".TargetApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.AddTargetTypeActivity" />
        <activity android:name=".ui.AddTargetManyTimeActivity" />

        <activity android:name=".ui.TargetFlowActivity" />

        <activity android:name=".ui.AboutActivity" />

        <activity android:name=".ui.AddItemActivity" />
        <activity android:name=".ui.RecordActivity" />
        <activity android:name=".ui.SettingActivity" />


        <activity android:name=".ui.CheckInActivity" />
    </application>

</manifest>