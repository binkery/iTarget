<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.binkery.itarget">

    <application
        android:name=".TargetApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.AddTargetActivity" />
        <activity android:name=".ui.activity.AddTargetTypeActivity" />
        <activity android:name=".ui.activity.AddTargetOneCountActivity" />
        <activity android:name=".ui.activity.AddTargetManyCountActivity" />
        <activity android:name=".ui.activity.AddTargetManyTimeActivity"/>

        <activity android:name=".ui.activity.ManyCountActivity" />
        <activity android:name=".ui.activity.OneCountActivity" />
        <activity android:name=".ui.activity.ManyTimeActivity" />
        <activity android:name=".ui.activity.BaseTargetDetailActivity"/>

        <activity android:name=".ui.TargetViewActivity" />
        <activity android:name=".ui.AddItemActivity" />
        <activity android:name=".ui.RecordActivity" />
        <activity android:name=".ui.SettingActivity" />

    </application>

</manifest>